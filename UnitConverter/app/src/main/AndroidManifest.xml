<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hr.ferit.mdudjak.unitconverter">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_unitconvert"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TemperatureConverterActivity" />
        <activity android:name=".WeightConverterActivity" />
        <activity android:name=".DistanceConverterActivity" />
        <activity android:name=".TimeConverterActivity" />
        <activity android:name=".DataReceiverActivity"></activity>
    </application>

</manifest>